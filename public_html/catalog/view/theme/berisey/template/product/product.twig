                    </div>
                  </div>
                </div>

                {% if (manufacture_info or products) %}
                  <div class="product-info-additional-block flex flex-col gap-2.5 mb-8 sm-tablet:hidden lg:flex">
                    {% if manufacture_info %}
                      <!-- product manufacture -->
                      <div id="product-manufacture-additional">
                        <a href="{{manufacturers}}" class="flex gap-3 justify-between">
                          <div class="flex flex-col">
                            <p class="font-semibold">{{text_manufacturer}}</p>
                            {{manufacturer}}
                          </div>
                          {% if manufacture_image %}
                            <div class="block">
                              <img src="{{manufacture_image}}" class="rounded-lg" alt="{{text_manufacturer}}{{manufacturer}}">
                            </div>
                          {% endif %}
                          
                        </a>
                      </div>
                      <!-- product manufacture -->
                    {% endif %}

                    {% if products %}
                      <!-- related products slider -->
                      <div id="related-products-slider-2">
                        <div class="swiper w-full rounded-lg" 
                          x-data="{ swiper: null }" 
                          x-ref="relatedSlider2"
                          x-init="swiper = new Swiper(
                            $refs.relatedSlider2, 
                            {
                              slidesPerView: 1.1,
                              effect: 'slide',
                              spaceBetween: 12,
                              direction: 'horizontal',
                              speed: 500,
                              passiveListeners: true,
                              loop: true,
                              keyboard: true,
                              lazy: true,
                              grabCursor: true,
                              parallax: true,
                              mousewheel : { enabled: true },
                            })">
                            <div class="swiper-wrapper">
        
                              {% for rel_product in products %}
                                <div class="swiper-slide w-full relative rounded-lg mb-10">
                                  <div class="relative flex gap-1 bg-lightBlue">
                                    <a href="{{rel_product.href}}" class="related-product-img max-w-[33.33%] w-full relative">
                                        <img src="{{rel_product.thumb}}" srcset="{{rel_product.thumb}}" class="absolute top-0 left-0 right-0 rounded-l-lg block w-full h-full object-cover" loading="lazy" alt="{{rel_product.name}}"/>
                                        <div class="swiper-lazy-preloader swiper-lazy-preloader-white"></div>
                                    </a>
                                    <div class="related-product-info flex flex-col p-1 gap-1 justify-between">
                                      <h4 class="text-xs text-darkGreen" data-swiper-parallax-duration="2000" data-swiper-parallax-opacity="0">{{ text_related }}</h4>
                                      <a href="{{rel_product.href}}" class="text-base line-clamp-2" data-swiper-parallax-opacity="0">{{rel_product.name}}</a>
                                      <div data-swiper-parallax-opacity="0" data-swiper-parallax-scale="0">
                                          {% if special %}
                                            <span class="text-base font-bold text-lightBlack pb-1">{{rel_product.special}}</span>
                                            <sup class="font-semibold text-sm line-through">{{rel_product.price}}</sup>
                                          {% else %}
                                            <span class="text-base font-bold text-lightBlack pb-1">{{rel_product.price}}</span>
                                          {% endif %}
                                      </div>
                                    </div>
                                    <!-- product actions  -->
                                    <div class="absolute bottom-0 right-0 translate-y-1/2 -translate-x-2">
                                      <button type="button" 
                                        onclick="cart.add('{{ rel_product.product_id }}', '{{ rel_product.minimum }}');"
                                        class="relative p-3 bg-white rounded-full shadow-sm group-hover/product-item:shadow-lg group/product-btn-cart transition hover:bg-gradient-to-r hover:from-lightGreen hover:to-darkGreen">
                                        <span class="absolute top-0 left-0 inline-flex h-full w-full transition group-hover/product-btn-cart:animate-ping rounded-full group-hover/product-btn-cart:bg-lightGreen opacity-75"></span>
                                        <svg class="w-5 h-5 fill-[url(#current_linear_gradient)] group-hover/product-btn-cart:fill-white transition" viewBox="0 0 18 18" xmlns="http://www.w3.org/2000/svg">
                                          <path fill-rule="evenodd" clip-rule="evenodd" d="M10.7999 0H7.19992V7.2H0V10.8H7.19992V18H10.7999V10.8H18V7.2H10.7999V0Z"/>
                                        </svg>
                                      </button>
                                    </div>
                                    <!-- product actions  -->
                                  </div>
                                </div>
                              {% endfor %}

                            </div>
        
                        </div>
                      </div>
                      <!-- related products slider -->
                    {% endif %}
                  </div>
                {% endif %}
  
                <div class="product-info-actions flex flex-col gap-5 mb-6">
