                      <span class="text-3xl font-bold text-lightBlack pb-1">{{special}}</span>
                      <sub class="ml-5 font-semibold text-xl line-through">{{price}}</sub>
                    {% else %}
                      <span class="text-3xl font-bold text-lightBlack pb-1">{{price}}</span>
                    {% endif %}
