	{{header}}
  <main id="main-content" class="max-w-full md:px-3 ">
    <div class="main-container main-bg-linear flex flex-col gap-20">
      <div class="main-container w-full max-w-screen-xl-monitor mx-auto grid gap-5 py-10 bg-white rounded-large">

        <nav class="breadcrumb-list flex justify-start px-8" aria-label="Breadcrumb">
          <ol class="inline-flex flex-wrap">
            {% for breadcrumb in breadcrumbs %}
              <li
                class="inline-flex items-start justify-end group/breadcrump-item before:block before:content-['\\'] before:px-2 first:before:content-none before:text-textGray">
                <a href="{{breadcrumb.href}}"
                  class="inline-flex items-center text-textGray min-h-6 hover:text-darkGreen group-last/breadcrump-item:hover:text-textGray group-last/breadcrump-item:pointer-events-none">
                  <svg class="w-4 h-4 me-2 hidden group-first/breadcrump-item:block" aria-hidden="true"
                    xmlns="http://www.w3.org/2000/svg" fill="currentColor" viewBox="0 0 20 20">
                    <path
                      d="m19.707 9.293-2-2-7-7a1 1 0 0 0-1.414 0l-7 7-2 2a1 1 0 0 0 1.414 1.414L2 10.414V18a2 2 0 0 0 2 2h3a1 1 0 0 0 1-1v-4a1 1 0 0 1 1-1h2a1 1 0 0 1 1 1v4a1 1 0 0 0 1 1h3a2 2 0 0 0 2-2v-7.586l.293.293a1 1 0 0 0 1.414-1.414Z" />
                  </svg>
                  <span
                    class="ms-1 text-sm font-medium group-last/breadcrump-item:font-semibold group-last/breadcrump-item:text-lightBlack group-last/breadcrump-item:pb-1 grid">
                    <span class="breadcrump-text">{{breadcrumb.text}}</span>
                  </span>
                </a>
              </li>
            {% endfor %}
          </ol>
        </nav>
  
        <section id="product" class="w-full py-8 lg:px-8">
          <div class="grid grid-cols-1 auto-rows-max gap-10 w-full max-w-screen-sm-monitor mx-auto 
              sm-tablet:grid-cols-2 sm-tablet:gap-x-3 
              md:grid-cols-[repeat(2,minmax(320px,_1fr))] md:grid-rows-[max-content]
              lg:grid-cols-[320px_minmax(320px,_1.5fr)_minmax(320px,_1fr)]
              xl-notebook:gap-x-10
              ">
            <div id="product-images" class="order-1 sm-tablet:col-span-1 sm-tablet:pl-6 sm-tablet:pt-1 lg:px-0" 
                style="--swiper-scrollbar-bg-color: transparent; --swiper-scrollbar-drag-bg-color: #313131;">

                  <div class="product-images-container w-full h-full flex flex-col ">

                    <div class="flex flex-col justify-center items-center w-full h-full gap-2 
                        xl-mobile:flex-row xl-mobile:items-start 
                        sm-tablet:flex-col sm-tablet:justify-start
                        lg:pb-10
                        xl-notebook:flex-row xl-notebook:h-auto">

                      <div class="swiper w-full max-w-[440px] xl-mobile:m-0 md:max-w-[calc(440px+24px)] xl-notebook:pr-6 lg:ml-auto" 
                        x-data="{ swiper: null }" 
                        x-ref="mainSlider"
                        x-init="swiper = new Swiper($refs.mainSlider, 
                        {
                          effect: 'slide',
                          spaceBetween: 24,
                          direction: 'horizontal',
                          fadeEffect: { crossFade: true },
                          speed: 1000,
                          autoplay: {delay: 5000,pauseOnMouseEnter: true},
                          loop: false,
                          passiveListeners: true,
                          keyboard: true,
                          lazy: true,
                          scrollbar: {el: '.swiper-scrollbar',draggable: true,},
                          grabCursor: true,
                          parallax: true,
                          observer: true,
                          resizeObserver: true,
                          breakpoints: {768: {direction : 'vertical',mousewheel : { enabled: {% if images %}true{% else %}false{% endif %}}}},
                          thumbs: {
                            swiper: new Swiper(document.querySelector('#thumbsSlider'),
                            {
                              autoHeight: true,
                              spaceBetween: 10,
                              slidesPerView: 'auto',
                              effect: 'slide',
                              direction: 'horizontal',
                              loop: false,
                              passiveListeners: true,
                              lazy: true,
                              grabCursor: true,
                              mousewheel : { enabled: false },
                              breakpoints: {480: {direction : 'vertical',},640: {direction : 'horizontal',},1280: {direction : 'vertical',},},
                            })
                          }
                        })">
                        <div class="swiper-wrapper h-0 pb-[115%] mb-6">
                            {% if (popup or images)  %}
                              {% if popup %}
                                <div class="swiper-slide w-full relative h-0 pb-[115%] overflow-hidden rounded-lg bg-lightBlue">
                                  <img src="{{thumb}}" srcset="{{popup}}" class="block absolute top-0 left-0 right-0 w-full h-full object-cover" loading="lazy" alt="{{heading_title}}"/>
                                  <div class="swiper-lazy-preloader swiper-lazy-preloader-white"></div>
                                </div>
                              {% endif %}

                              {% if images %}
                                {% for image in images %}
                                  <div class="swiper-slide w-full relative h-0 pb-[115%] overflow-hidden rounded-lg bg-lightBlue">
                                    <img src="{{image.thumb}}" srcset="{{image.popup}}" class="block absolute top-0 left-0 right-0 w-full h-full object-cover" loading="lazy" alt="{{heading_title}}"/>
                                    <div class="swiper-lazy-preloader swiper-lazy-preloader-white"></div>
                                  </div>
                                {% endfor %}
                              {% endif %}

                            {% else %}
                              <div class="swiper-slide w-full relative h-0 pb-[115%] overflow-hidden rounded-lg bg-lightBlue">
                                <img src="{{image_placeholder.thumb}}" srcset="{{image_placeholder.popup}}" class="block absolute top-0 left-0 right-0 w-full h-full object-contain" loading="lazy" alt="{{heading_title}}"/>
                                <div class="swiper-lazy-preloader swiper-lazy-preloader-white"></div>
                              </div>
                            {% endif %}

                      </div>
        
                      <div class="swiper-scrollbar transition-all hover:w-3 cursor-grab"></div>
        
                    </div>
                    <div id="thumbsSlider" 
                      {% if images %}
                        class="hidden swiper w-full h-full max-w-[440px] mx-auto [&.swiper-horizontal>.swiper-wrapper]:py-1 [&.swiper-horizontal_.swiper-slide]:w-20
                          xl-mobile:block xl-mobile:w-24 xl-mobile:m-0 
                          sm-tablet:w-full sm-tablet:h-auto
                          xl-notebook:w-24 xl-notebook:h-full
                          lg:mr-auto"
                      {% else %}
                        class="w-0 h-0 opacity-0 lg:mr-auto"
                      {% endif %}>
                      <div class="swiper-wrapper">
                          {% if (popup or images) %}
                            {% if popup %}
                              <div class="swiper-slide pb-20 relative overflow-hidden rounded-md bg-lightBlue [&.swiper-slide-thumb-active]:border [&.swiper-slide-thumb-active]:border-darkGreen">
                                <img src="{{thumb}}" srcset="{{popup}}" class="block absolute p-1 rounded-md top-0 left-0 right-0 w-full h-full object-cover" loading="lazy" alt="{{heading_title}}"/>
                                <div class="swiper-lazy-preloader swiper-lazy-preloader-white"></div>
                              </div>
                            {% endif %}
                            {% if images %}
                              {% for image in images %}
                                <div class="swiper-slide pb-20 relative overflow-hidden rounded-md bg-lightBlue [&.swiper-slide-thumb-active]:border [&.swiper-slide-thumb-active]:border-darkGreen">
                                  <img src="{{image.thumb}}" srcset="{{image.popup}}" class="block absolute p-1 rounded-md top-0 left-0 right-0 w-full h-full object-cover" loading="lazy" alt="{{heading_title}}"/>
                                  <div class="swiper-lazy-preloader swiper-lazy-preloader-white"></div>
                                </div>
                              {% endfor %}
                            {% endif %}
                          {% endif %}
                      </div>
                    </div>
                </div>
  
                <div class="hidden w-full pt-10 sm-tablet:block lg:hidden">
                  <div class="product-info-additional-block hidden flex-col gap-2.5 sm-tablet:flex">
                    {% if manufacture_info %}
                      <!-- product manufacture -->
                      <div id="product-manufacture-images">
                        <a href="{{manufacturers}}" class="flex gap-3 justify-between">
                          <div class="flex flex-col">
                            <p class="font-semibold">{{text_manufacturer}}</p>
                            {{manufacturer}}
                          </div>
                          {% if manufacture_image %}
                            <div class="block">
                              <img src="{{manufacture_image}}" class="rounded-lg" alt="{{text_manufacturer}}{{manufacturer}}">
                            </div>
                          {% endif %}
                        </a>
                      </div>
                      <!-- product manufacture -->
                    {% endif %}

                    {% if products %}
                      <!-- related products slider -->
                      <div id="related-products-slider-1">
                        <div class="swiper w-full rounded-lg" 
                          x-data="{ swiper: null }" 
                          x-ref="relatedSlider1"
                          x-init="swiper = new Swiper(
                            $refs.relatedSlider1, 
                            {
                              slidesPerView: 1.1,
                              effect: 'slide',
                              spaceBetween: 12,
                              direction: 'horizontal',
                              speed: 500,
                              passiveListeners: true,
                              loop: true,
                              keyboard: true,
                              lazy: true,
                              grabCursor: true,
                              parallax: true,
                            })">
                          <div class="swiper-wrapper">
                            {% for rel_product in products %}
                              <div class="swiper-slide w-full relative rounded-lg mb-10">
                                <div class="relative flex gap-1 bg-lightBlue">
                                  <a href="{{rel_product.href}}" class="related-product-img max-w-[33.33%] w-full relative">
                                      <img src="{{rel_product.thumb}}" srcset="{{rel_product.thumb}}" class="absolute top-0 left-0 right-0 rounded-l-lg block w-full h-full object-cover" loading="lazy" alt="{{rel_product.name}}"/>
                                      <div class="swiper-lazy-preloader swiper-lazy-preloader-white"></div>
                                  </a>
                                  <div class="related-product-info flex flex-col p-1 gap-1 justify-between">
                                    <h4 class="text-xs text-darkGreen" data-swiper-parallax-duration="2000" data-swiper-parallax-opacity="0">{{ text_related }}</h4>
                                    <a href="{{rel_product.href}}" class="text-base line-clamp-2" data-swiper-parallax-opacity="0">{{rel_product.name}}</a>
                                    <div data-swiper-parallax-opacity="0" data-swiper-parallax-scale="0">
                                        {% if special %}
                                          <span class="text-base font-bold text-lightBlack pb-1">{{rel_product.special}}</span>
                                          <sup class="font-semibold text-sm line-through">{{rel_product.price}}</sup>
                                        {% else %}
                                          <span class="text-base font-bold text-lightBlack pb-1">{{rel_product.price}}</span>
                                        {% endif %}
                                    </div>
                                  </div>
                                  <!-- product actions  -->
                                  <div class="absolute bottom-0 right-0 translate-y-1/2 -translate-x-2">
                                    <button type="button" 
                                      onclick="cart.add('{{ rel_product.product_id }}', '{{ rel_product.minimum }}');"
                                      class="relative p-3 bg-white rounded-full shadow-sm group-hover/product-item:shadow-lg group/product-btn-cart transition hover:bg-gradient-to-r hover:from-lightGreen hover:to-darkGreen">
                                      <span class="absolute top-0 left-0 inline-flex h-full w-full transition group-hover/product-btn-cart:animate-ping rounded-full group-hover/product-btn-cart:bg-lightGreen opacity-75"></span>
                                      <svg class="w-5 h-5 fill-[url(#current_linear_gradient)] group-hover/product-btn-cart:fill-white transition" viewBox="0 0 18 18" xmlns="http://www.w3.org/2000/svg">
                                        <path fill-rule="evenodd" clip-rule="evenodd" d="M10.7999 0H7.19992V7.2H0V10.8H7.19992V18H10.7999V10.8H18V7.2H10.7999V0Z"/>
                                      </svg>
                                    </button>
                                  </div>
                                  <!-- product actions  -->
                                </div>
                              </div>
                            {% endfor %}
                          </div>
        
                        </div>
                      </div>
                      <!-- related products slider -->
                    {% endif %}
                    
                  </div>
                </div>
  
              </div>
            </div>
  
            <div id="product-info" class="sm-tablet:col-span-1 order-2">
              <div class="product-info-container w-full h-full flex flex-col justify-between p-6 max-w-[440px] mx-auto xl-mobile:max-w-[calc(440px+0.75rem+6rem+1.5rem)] sm-tablet:py-0">
  
                <div class="product-info-main-block flex flex-col gap-6 mb-6">
                  <!-- product title -->
                  <h1 id="product-name">
                    <div class="text-2xl text-lightBlack uppercase font-bold line-clamp-3">{{heading_title}}</div>
                    <p class="text-darkGreen text-sm">{{text_model}} {{model}}</p>
                  </h1>
                  <!-- product title -->
                  
                  <!-- product attributes  -->
                  <ul class="grid grid-cols-2 gap-3 text-xs md:text-xs lg:text-base font-semibold">

                    <li class="flex items-center" data-tooltip-target="tooltip-stock" x-data="{display: {{config_stock_display}}}">
                      <svg class="w-6 h-6 float-left mr-3" viewBox="0 0 12 11" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <path fill-rule="evenodd" clip-rule="evenodd" d="M4.78474 0.788443L3.58121 1.57527V2.20522V2.83517H3.75685H3.93248L3.93884 2.31662L3.94521 1.79807L4.94325 1.14622C5.49217 0.787674 5.96243 0.49005 5.98826 0.484801C6.01754 0.478835 6.41521 0.723992 7.04501 1.13625L8.05479 1.79728L8.06116 2.31622L8.06752 2.83517H8.24315H8.41879V2.20442V1.57368L7.21061 0.78603C6.54611 0.352824 5.99925 -0.00089969 5.99535 1.71901e-06C5.99145 0.000903128 5.44668 0.355687 4.78474 0.788443ZM5.86497 0.849553C5.79717 0.894173 5.74168 0.93585 5.74168 0.94216C5.74168 0.96716 5.88848 1.24543 5.90933 1.25999C5.92159 1.26852 5.94547 1.26256 5.9624 1.24668C5.98579 1.22475 6.11028 1.29432 6.48031 1.536C6.7482 1.71098 6.97595 1.84821 6.98635 1.84094C7.00499 1.82793 7.15068 1.54801 7.15068 1.52521C7.15068 1.51447 6.88826 1.33981 6.25508 0.929222C6.1177 0.840141 6.00146 0.767498 5.99676 0.767817C5.99209 0.768135 5.93277 0.804907 5.86497 0.849553ZM5.35994 1.18111L5.22474 1.27544L5.28505 1.38586C5.31821 1.4466 5.35688 1.52298 5.371 1.55557L5.39667 1.61485L5.52808 1.53171C5.60036 1.486 5.66863 1.43947 5.6798 1.42831C5.70383 1.40432 5.55386 1.08506 5.51904 1.08609C5.50589 1.08649 5.43429 1.12923 5.35994 1.18111ZM4.87827 1.49446C4.81024 1.54215 4.74838 1.58815 4.74082 1.59669C4.72469 1.6149 4.87569 1.93376 4.90044 1.93376C4.90969 1.93376 4.98052 1.8919 5.05787 1.84071L5.19849 1.74767L5.11198 1.57585C5.06438 1.48133 5.02016 1.40485 5.0137 1.40588C5.00724 1.40689 4.9463 1.44676 4.87827 1.49446ZM4.22813 1.98819C4.18199 2.07711 4.14951 2.15951 4.15599 2.17136C4.16247 2.18319 4.53776 2.43394 4.98996 2.72857L5.81213 3.26429L5.81117 4.03731L5.81021 4.81032L4.70158 5.53036L3.59295 6.25037L3.58711 7.8369L3.58124 9.42341L4.79138 10.2117L6.0015 11L7.21033 10.2119L8.41914 9.42375L8.4131 7.83629L8.40705 6.24886L7.30333 5.52911L6.19961 4.80934L6.19339 4.03799L6.18716 3.26663L7.0153 2.72822C7.47079 2.43208 7.84765 2.18608 7.85281 2.18157C7.86427 2.17147 7.76283 1.95267 7.71429 1.88281C7.68054 1.834